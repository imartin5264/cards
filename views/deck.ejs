<% if(typeof deckname != 'undefined'){%>
    <% deckname.forEach(function(deck){%>
        <div>
            <a href="<%= `https://social-flashcards.herokuapp.com/card/${deck}/false/0/0/noError` %>">
                <button>
                    <%= deck %>
                </button>
            </a>
            
            <br>
        </div>
        
    <%});%>
<%}%>